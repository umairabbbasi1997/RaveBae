<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fictivestudios.ravebae" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RaveBae" >
        <activity
            android:name=".activites.ResgistrationActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SignupActivity"
            android:exported="false" />
        <activity
            android:name=".activites.PreLoginActivity"
            android:exported="false" />
        <activity
            android:name=".activites.MainActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.App.Starting" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>