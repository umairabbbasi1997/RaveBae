        android:id="@+id/settingsFragment"
        android:name="com.fictivestudios.ravebae.fragments.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/agreementFragment"
        android:name="com.fictivestudios.ravebae.fragments.AgreementFragment"
        android:label="fragment_agreement"
        tools:layout="@layout/fragment_agreement" />
    <fragment
        android:id="@+id/changePassowrdFragment"
        android:name="com.fictivestudios.ravebae.fragments.ChangePassowrdFragment"
        android:label="fragment_change_passowrd"
        tools:layout="@layout/fragment_change_passowrd" />
    <fragment
        android:id="@+id/forgotFragment"
        android:name="com.fictivestudios.ravebae.fragments.ForgotFragment"
        android:label="fragment_forgot"
        tools:layout="@layout/fragment_forgot" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.fictivestudios.ravebae.fragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" />
    <fragment
        android:id="@+id/otpFragment"
        android:name="com.fictivestudios.ravebae.fragments.OtpFragment"
        android:label="fragment_otp"
        tools:layout="@layout/fragment_otp" />
    <fragment
        android:id="@+id/preLoginFragment"
        android:name="com.fictivestudios.ravebae.fragments.PreLoginFragment"
        android:label="fragment_pre_login"
        tools:layout="@layout/fragment_pre_login" />
    <fragment
        android:id="@+id/privacyAndPolicyFragment"
        android:name="com.fictivestudios.ravebae.fragments.PrivacyAndPolicyFragment"
        android:label="fragment_privacy_and_policy"
        tools:layout="@layout/fragment_privacy_and_policy" />
    <fragment
        android:id="@+id/resetPasswordFragment"
        android:name="com.fictivestudios.ravebae.fragments.ResetPasswordFragment"
        android:label="fragment_reset_password"
        tools:layout="@layout/fragment_reset_password" />
    <fragment
        android:id="@+id/signupFragment"
        android:name="com.fictivestudios.ravebae.fragments.SignupFragment"
        android:label="fragment_signup"
        tools:layout="@layout/fragment_signup" />
    <fragment
        android:id="@+id/termsAndConditionsFragment"
        android:name="com.fictivestudios.ravebae.fragments.TermsAndConditionsFragment"
        android:label="fragment_terms_and_conditions"
        tools:layout="@layout/fragment_terms_and_conditions" />
    <fragment
        android:id="@+id/userProfileDetailFragment"
        android:name="com.fictivestudios.ravebae.fragments.userProfileDetailFragment"
        android:label="fragment_user_profile_detail"
        tools:layout="@layout/fragment_user_profile_detail" />
    <fragment
        android:id="@+id/discoverFragment2"
        android:name="com.fictivestudios.ravebae.fragments.DiscoverFragment"
        android:label="fragment_discover"
        tools:layout="@layout/fragment_discover" />
    <fragment
        android:id="@+id/loginWithPhoneFragment"
        android:name="com.fictivestudios.ravebae.fragments.LoginWithPhoneFragment"
        android:label="fragment_login_with_phone"
        tools:layout="@layout/fragment_login_with_phone" />
</navigation>